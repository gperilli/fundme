<div class="aa-innercontainer" style="width: 100%; height: 100%; padding: 10px;">

    <table>
        <tr>
        <th>Donator</th>
        <th>Donation Type</th>
        <th>Amount</th>
        <th>Time</th>
        </tr>

        <%  Donation.all.order(created_at: :desc).first(5).map do |donation| %>
            <tr class="spaceUnder">
                <td><%= donation.user.first_name %> <%= donation.user.last_name %></td>
                <td><%= donation.donation_type %></td>
                <td><%= donation.amount %></td>
                <td><%= donation.created_at %></td>
            </tr>
        <% end %>

    </table>


</div>